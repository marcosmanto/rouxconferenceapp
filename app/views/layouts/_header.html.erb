<div class="content row">
	<div class="col-lg-12">
		<header class="clearfix">
			<section id="branding">
				<%= link_to(root_path) do  %>
					<%= image_tag("misc/ralogo_monogram.png", alt: "Logo for Roux Conference") %>
				<% end %>
			</section>
			<div class="navbar navbar-default">
				<ul class="nav navbar-nav">
					<li><%= nav_link 'Home', root_path, 'home'  %></li>
					<li><a href="#">Venue/Travel</a></li>
					<li><a href="#">Schedule</a></li>
					<li class="dropdown">
						<a href="#" class="dropdown-toggle" data-toggle="dropdown">Artists <span class="caret"></span></a>
						<ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu">
							<li><%= link_to "All artists", artists_path, tabindex: "-1" %></li>
							<li class="divider"></li>
							<% @artists.each do |artist| %>
								<!-- <li><%#= link_to artist.name, artist_path(artist), tabindex: "-1"  %></li> -->
								<li><%= link_to artist.name, artists_path(anchor: "#{artist.name.parameterize.underscore}"), tabindex: "-1"  %></li>
							<% end %>
						</ul>
					</li>
					<li><a href="#">Register</a></li>
				</ul>
			</div>
		</header>
	</div>
</div>